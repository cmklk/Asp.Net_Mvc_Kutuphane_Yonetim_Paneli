
@using MVCKUTUPHANE.Models.Entity
@model List<tbl_duyurular>




@foreach (var x in Model)

{
    <div class="post">
        <div class="user-block">
            <img class="img-circle img-bordered-sm" src="~/AdminLTE-3.0.4/dist/img/user1-128x128.jpg" alt="user image">
            <span class="username">
                <a href="#">@x.KATEGORI</a>
            </span>
            <span class="description">@x.TARİH</span>
        </div>
        <!-- /.user-block -->
        <p>
          @x.ICERIK
        </p>
    </div>
}